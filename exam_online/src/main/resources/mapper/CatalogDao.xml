<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wzy.exam_online.dao.CatalogDao">
<!--    查询所有的记录-->
    <select id="searchCatalogsAll"  resultType="catalog">
        select * from catalog
    </select>


    <!--    新增一个记录-->
    <!-- useGeneratedKeys：  主键是否回填   -->
    <!-- keyProperty：  主键回填接收的变量名  -->
    <insert id="insertOneCatalog" parameterType="catalog" useGeneratedKeys="true" keyProperty="id">
        insert into catalog values (null,#{cname},#{course},#{major},#{createTime})
    </insert>

</mapper>